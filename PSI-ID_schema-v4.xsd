    <!--features-->
    
    <xs:complexType name="featureList">
        <xs:annotation>
            <xs:documentation>Sequence features of an experimental construct relevant for the structure state of the region, such as 
                PTM, or for instance experimental modifications, e.g. protein tags.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="feature" type="idf:feature" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="feature">
        <xs:annotation>
            <xs:documentation>
                A feature on a sequence of an experimental construct, or any modifications made to the source sequence.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="names" type="idf:names" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Name(s) for the feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="xref" type="idf:xref" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Reference to an external description of the feature, e.g. PSI-MOD, ELM, interpro.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="featureType" type="idf:cvType" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The type of feature, controlled by the PSI-ID controlled vocabulary.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="featureDetectionMethodList" type="idf:featureDetectionMethodList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Experimental method(s) used to identify the feature, controlled by the ECO CV.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="featureRangeList">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="featureRange" type="idf:baseLocation" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>
                                    Location (start and end position) of the feature on the sequence of the experimental construct, i.e. with
                                    respect to the sequence given for the experimental construct. One feature may have more than one 
                                    featureRange, used for instance for features which involve sequence positions close in the folded, 
                                    three-dimensional state of a protein, but non-continuous along the sequence.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="attributeList" type="idf:attributeList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Semi-structured additional description of the data contained in the entry.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:int" use="required"/>
    </xs:complexType>
    
    
    <!--CV type-->
    
    <xs:complexType name="cvType">
        <xs:annotation>
            <xs:documentation>
                Reference to an external controlled vocabulary.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="names" type="idf:names">
                <xs:annotation>
                    <xs:documentation>
                        Name of the controlled vocabulary term.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="xref" type="idf:xref">
                <xs:annotation>
                    <xs:documentation>
                        Source of the controlled vocabulary term, e.g. the name of the CV and the term ID. The name of the CV and the 
                        type of reference are controlled by the PSI-ID CV.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="openCvType">
        <xs:annotation>
            <xs:documentation>
                Allows to reference an external controlled vocabulary, or to directly include a value if no suitable external 
                definition is available.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="names" type="idf:names">
                <xs:annotation>
                    <xs:documentation>
                        This contains the controlled vocabulary terms, as a short and optionally as a long 
                        form.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="xref" type="idf:xref" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Refers to the term of the controlled vocabulary in an external database (the name of the CV and the term ID). 
                        The name of the CV and the type of reference are controlled by the PSI-ID CV.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="attributeList" type="idf:attributeList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        If no suitable external controlled vocabulary is available, this attributeList can be used to describe the 
                        term. Example: Attribute name: Mouse atlas tissue name; attribute value: spinal cord, day 30.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    
    <!--names-->
    
    <xs:complexType name="names">
        <xs:annotation>
            <xs:documentation>
                Name(s) for an object.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="shortLabel" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        A short string, suitable to remember the object. Can be e.g. a protein name, the first author of a paper, etc.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="fullName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        A full, detailed name or description of the object. Can be e.g. the full title of a publication, or the 
                        scientific name of a species.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="alias" type="idf:alias" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="fullName">
        <xs:annotation>
            <xs:documentation>
                Full, descriptive object name.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string"/>
        </xs:simpleContent>
    </xs:complexType>
    
    <!-- check if this is still used/necessary
    <xs:complexType name="label">
        <xs:annotation>
            <xs:documentation>
                A short alphanumeric label identifying an object. Not necessarily unique.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string"/>
        </xs:simpleContent>
    </xs:complexType>
    -->
    
    <xs:complexType name="alias">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="typeAc" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="type" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    
    <!--source/host organism-->
    
    <xs:complexType name="bioSource">
        <xs:annotation>
            <xs:documentation>
                Describes the biological source of an object, in simple form only the NCBI taxid.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="names" type="idf:names" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The name(s) of the organism. The short label should be a common name if it exists. The full name should be the 
                        full name of the species (i.e. genus species).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cellType" type="idf:openCvType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Description of the cell type. Currently no species-independent controlled vocabulary for cell types (cell 
                        ontology?) is available, therefore the choice of reference database(s) is open to the data provider.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="compartment" type="idf:openCvType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The subcellular compartment of the object. It is strongly recommended to refer to the Gene Ontology cellular 
                        component in this element.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="tissue" type="idf:openCvType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Description of the source tissue. Currently no species-independent controlled vocabulary for tissues (BRENDA?) 
                        is available, therefore the choice of reference database(s) is open to the data provider.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="ncbiTaxId" type="xs:int" use="required"/>
    </xs:complexType>
    
    
    <!--bibliographic and cross reference, both using dbRef type-->
    
    <xs:complexType name="bibref">
        <xs:annotation>
            <xs:documentation>
                Bibliographic reference. The PubMed primary reference is added to the xref element of bibref and publication details 
                (e.g. title) can be described in the attributeList of the bibref element. The type of bibliographic attribute is 
                controlled by the PSI-ID CV.
            </xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="1" maxOccurs="1">
            <xs:sequence>
                <xs:element name="xref" type="idf:xref" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Bibliographic reference in an external database, usually PubMed. The name of the literature database is 
                            controlled by the PSI-ID.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="attributeList" type="idf:attributeList" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Additional description of the bibliographic reference such as publication title, authors, journal, 
                            publication date. The type of bibliographic attribute is controlled by the PSI-ID CV.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:sequence>
                <xs:element name="attributeList" type="idf:attributeList" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Alternative description of the bibliographic reference if no external database entry is available.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:choice>
    </xs:complexType>
    
    <xs:complexType name="xref">
        <xs:annotation>
            <xs:documentation>
                Cross-reference to an external database. Cross-references to literature databases (e.g. PubMed) should not be put into 
                this structure, but into the bibRef element.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="primaryRef" type="idf:dbReference">
                <xs:annotation>
                    <xs:documentation>
                        Primary reference to an external database. The name of the external database and the type of reference are 
                        controlled by the PSI-ID CV.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="secondaryRef" type="idf:dbReference" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Further external objects describing the object.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="dbReference">
        <xs:annotation>
            <xs:documentation>
                Refers to a unique object in an external database, e.g. a protein in UniProt or an entry in DisProt.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element name="attributeList" type="idf:attributeList"/>
        </xs:sequence>
        <xs:attribute name="db" use="required">
            <xs:annotation>
                <xs:documentation>
                    Name of the external database, controlled by the PSI-ID CV.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="dbAc" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Accession number of the term for the database in the PSI-ID CV (database citation branch).
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="id" use="required">
            <xs:annotation>
                <xs:documentation>
                    Primary identifier of the object in the external database, e.g. UniProt accession number.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="secondary" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Secondary identifier of the object in the external database, e.g. UniProt ID.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="version" use="optional">
            <xs:annotation>
                <xs:documentation>
                    The version number of the object in the external database.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="refType" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Reference type, e.g. "identity" if this reference refers to an identical object in the external database, or 
                    "see-also" for additional information, controlled by the PSI-ID CV (cross-reference type branch).
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="refTypeAc" use="optional">
            <xs:annotation>
                <xs:documentation>
                    ID of the reference type term from the PSI-ID CV.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    
    <!--within entry references-->
    
    <!-- check if this is still used/necessary
    <xs:complexType name="experimentRefList">
        <xs:annotation>
            <xs:documentation>
                Refers to a list of experiments within the same entry.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="experimentRef" type="xs:int" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        References an experiment already described in this entry.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    -->
    
    <!--experimental identification of molecule/experimental construct-->
    
    <xs:complexType name="moleculeIdentificationMethodList">
        <xs:annotation>
            <xs:documentation>
                The method(s) by which this experimental construct has been determined/identified. If this element is present, its 
                value supersedes experimentDescription/moleculeIdentificationMethod. This element is controlled by the ECO ontology.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="moleculeIdentificationMethod" type="idf:cvType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <!--experimental preparation of regions-->
    
    <xs:complexType name="experimentalPreparationList">
        <xs:annotation>
            <xs:documentation>
                Terms describing the experimental preparation of an experimental construct, controlled by the PSI-ID CV.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="experimentalPreparation" type="idf:cvType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <!--experimental detection of feature-->
    
    <xs:complexType name="featureDetectionMethodList">
        <xs:annotation>
            <xs:documentation>
                The method(s) by which the feature has been determined/identified. If this element is present, its value supersedes 
                experimentDescription/moleculeIdentificationMethod. This element is controlled by the ECO ontology.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="featureDetectionMethod" type="idf:cvType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <!--location of a feature on a sequence-->
    
    <xs:complexType name="baseLocation">
        <xs:annotation>
            <xs:documentation>
                A location on a sequence defined by a start and end position. Both begin and end can be a defined position, a fuzzy 
                position, or undetermined. The start and stop position of an experimental construct is based on the sequence given for 
                the source molecule (molecule element). The start and stop positions of regions of a molecule or experimental construct 
                for which a structure state was inferred, as well as the start and stop positions of features on the experimental construct 
                are mapped to the sequence given for the experimental construct (experimentalConstruct element).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:sequence>
                <xs:element name="startStatus" type="idf:cvType">
                    <xs:annotation>
                        <xs:documentation>
                            Attribute of the start positions, e.g. "certain" or "n-terminal", controlled by the PSI-ID CV.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:choice minOccurs="0">
                    <xs:element name="begin" type="idf:position">
                        <xs:annotation>
                            <xs:documentation>
                                The integer position gives the begin position of the region/feature, with respect to the sequence of 
                                the experimental construct, which itself is mapped to the sequence of the source molecule. The first 
                                base or amino acid of the reference sequence is position 1. In combination with the numeric value, the 
                                attribute 'status' allows to express fuzzy positions, e.g. 'less than 4'.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="beginInterval" type="idf:interval">
                        <xs:annotation>
                            <xs:documentation>
                                The begin position may be varying or unclear, but localisable to a certain range. Usually written as 
                                e.g. 3..5.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:sequence>
            <xs:sequence>
                <xs:element name="endStatus" type="idf:cvType">
                    <xs:annotation>
                        <xs:documentation>
                            Attribute of the end positions, e.g. "certain" or "c-terminal", controlled by the PSI-ID CV.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:choice minOccurs="0">
                    <xs:element name="end" type="idf:position">
                        <xs:annotation>
                            <xs:documentation>
                                The integer position gives the end position of the region/feature, with respect to the sequence of the 
                                experimental construct, which itself is mapped to the sequence of the source molecule. The first base 
                                or amino acid of the reference sequence is position 1. In combination with the numeric value, the 
                                attribute 'status' allows to express fuzzy positions, e.g. 'more than 400'.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="endInterval" type="idf:interval">
                        <xs:annotation>
                            <xs:documentation>
                                The end position may be varying or unclear, but localisable to a certain range. Usually written as e.g.
                                3..5.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:sequence>
            <xs:element name="isLink" type="xs:boolean" default="false" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        True if the described feature is a linking feature connecting two amino acids rather than extending along the 
                        sequence. 'begin' references the first amino acid, 'end' the second. Standard example is a disulfide bridge. 
                        Does not reference another feature, therefore is only suitable for linking features on the same amino acid 
                        chain.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="position">
        <xs:attribute name="position" type="xs:long" use="required"/>
    </xs:complexType>
    
    <xs:complexType name="interval">
        <xs:annotation>
            <xs:documentation>
                An interval on a sequence.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="begin" type="xs:long" use="required"/>
        <xs:attribute name="end" type="xs:long" use="required"/>
    </xs:complexType>
    
    
    <!--attributelist and attribute-->
    
    <xs:complexType name="attributeList">
        <xs:annotation>
            <xs:documentation>
                A list of additional attributes. Open tag-value list to allow the inclusion of additional (annotation of) data.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="attribute" type="idf:attribute" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="attribute">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="name" use="required">
                    <xs:annotation>
                        <xs:documentation>
                            The name of the attribute, controlled by the PSI-ID CV.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="nameAc" use="optional">
                    <xs:annotation>
                        <xs:documentation>
                            Enables control of the attribute type by using terms from the PSI-ID CV (attribute name branch).
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
 
</xs:schema>